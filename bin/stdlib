# function declaration, Bashpile line 16
toBashpileOutputFile() {
  declare -r inFilename=$1
  # assign statement, Bashpile line 18
  declare outFilename
  outFilename=$(printf -- "$inFilename" | gsed -E 's/\..[^.]?[^.]?[^.]?[^.]?$//g')
  if [ "${inFilename}" == "${outFilename}" ]; then
    # reassign statement, Bashpile line 21
    outFilename=${inFilename}.bash
  fi
  # return statement, Bashpile line 22
  printf -- "${outFilename}"
}
