assertGnuGetopt() {
  # no parameters to function
  if [ "$(getopt --help)" == " --" ]; then
    # print statement, Bashpile line 8
    printf -- "FreeBSD getopt found.  Please ensure that brew's gnu-getopt is first on the classpath.  Exiting...\n"
    exit 2
  fi
}
# function declaration, Bashpile line 14
createTempFile() {
  set +u
  declare -r filename=$1
  set -u
  # assign statement, Bashpile line 15
  declare ret
  if [ -n "${filename+default}" ] && [ -n "${filename}" ]; then
    # reassign statement, Bashpile line 17
    ret=$(mktemp -t $filename)
  else
    # reassign statement, Bashpile line 19
    ret=$(mktemp)
  fi
  # return statement, Bashpile line 20
  printf -- "${ret}"
}
# function declaration, Bashpile line 22
toBashpileOutputFile() {
  set +u
  declare -r inFilename=$1
  set -u
  # assign statement, Bashpile line 24
  declare outFilename
  outFilename=$(printf -- "$inFilename" | gsed -E 's/\..[^.]?[^.]?[^.]?[^.]?$//g')
  if [ "${inFilename}" == "${outFilename}" ]; then
    # reassign statement, Bashpile line 27
    outFilename=${inFilename}.bash
  fi
  # return statement, Bashpile line 28
  printf -- "${outFilename}"
}
